#include <iostream>

using namespace std;

int main()
{
	int a; //declaring the variable//
	int b;
	int sum;
	
	cout << "Enter a number! \n";
	cin >> a; // cin-input stream object, >>stream- extraction operator
	
	cout << "Enter another number! \n";
	cin >> b;
	
	sum = a + b;
	cout << "the sum of those numers is " << sum << endl;
	
	return 0;
}
