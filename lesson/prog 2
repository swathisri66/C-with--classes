/* program to accept 2 inputs and print their sum */
#include <iostream>
using namespace std;
int main()
{
	int x,y,z;
	cout<<"\n enter 2 values ";
	cin>>x>>y;
	z=x+y;
	cout<<"\n total is "<<z;
	cout<<"\n "<<z;
	cout<<"\n"<<z<<" is the total";	
	// total of 40 and 50 is 90
	cout<<"\n total of "<<x<<" and "<<y<<" is "<<z;
	cout<<"\n total of "<<x<<" and "<<y<<" is "<<(x+y);
}
